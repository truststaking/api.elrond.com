{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "../../",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "modulePaths": [ "<rootDir>" ],
  "collectCoverageFrom": [
    "./src/**/*.(t|j)s"
  ],
  "coverageDirectory": "./coverage/e2e"
}
